      "name": "Brise",
      "shortcut": "BRISE",
      "label": "Brise",
      "alias": "Brise"
  },
  "ports": {
      "backend_rpc": 8064,
      "backend_message_queue": 0,
      "blockbook_internal": 9064,
      "blockbook_public": 9164
  },
  "ipc": {
    "rpc_url_template": "ws://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_timeout": 25
  },
  "backend": {
    "package_name": "backend-brise",
    "package_revision": "satoshilabs-1",
    "system_user": "brise",
    "version": "1.10.17-25c9b49f",
    "binary_url": "https://gethstore.blob.core.windows.net/builds/geth-linux-amd64-1.10.17-25c9b49f.tar.gz",
    "verification_type": "gpg",
    "verification_source": "https://gethstore.blob.core.windows.net/builds/geth-linux-amd64-1.10.17-25c9b49f.tar.gz.asc",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [],
    "exec_command_template": "/bin/sh -c '{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/geth --datadir {{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend --port 40605 --ws --ws.addr 127.0.0.1 --ws.port {{.Ports.BackendRPC}} --ws.origins \"*\" --http --http.port 8164 -http.addr 127.0.0.1 --http.corsdomain \"*\" --http.vhosts \"*\" --bootnodes enode://bab5d43f3ad9612f909547fa754b955c0509a11a55c7f067b5bdc434b5fb6cea363c193c6c00557bc90267da7aef3a5997c376754f6df2d6817cf52c90d1852e@54.251.84.204:40605 --networkid 32520  2>>{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/{{.Coin.Alias}}.log'",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/{{.Coin.Alias}}.log",
    "postinst_script_template": "",
    "service_type": "simple",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "geth_init_url": "https://raw.githubusercontent.com/Bitgert/brise-chain/master/brise_mainnet.json",
    "server_config_file": "",
    "client_config_file": ""
  },
  "blockbook": {
      "package_name": "blockbook-brise",
      "system_user": "blockbook-brise",
      "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
      "public_binding_template": ":{{.Ports.BlockbookPublic}}",
      "explorer_url": "",
      "additional_params": "",
      "block_chain": {
          "parse": true,
          "mempool_workers": 8,
          "mempool_sub_workers": 2,
          "block_addresses_to_keep": 300,
          "additional_params": {
              "mempoolTxTimeoutHours": 48,
              "queryBackendOnMempoolResync": false,
              "fiat_rates": "coingecko",
              "fiat_rates_params": "{\"url\": \"https://api.coingecko.com/api/v3\", \"coin\": \"brise\", \"periodSeconds\": 60}"
          }
      }
  },
  "meta": {
      "package_maintainer": "IT",
      "package_maintainer_email": "it@satoshilabs.com"
  }
